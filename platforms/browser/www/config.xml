<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.mitchellansems.BrainUp" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BrainUp</name>
    <description>
        A simple application to train the brain.
    </description>
    <author email="m.p.j.ansems@student.tue.nl" href="http://mitchellansems.nl">
        Mitchell Ansems
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
</widget>
